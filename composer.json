{
    "name": "jonijarvinen/ajo-tdd-examples-marketplace",
    "description": "Alma Ajo TDD Examples, Marketplace",
    "type": "project",
    "minimum-stability": "stable",
    "prefer-stable": true,
    "authors": [
        {
            "name": "Joni JÃ¤rvinen",
            "email": "joni.jarvinen@almamedia.fi"
        }
    ],
    "repositories": [
        {
            "type": "path",
            "url": "Common"
        }
    ],
    "require": {
        "jonijarvinen/ajo-tdd-examples-common": "dev-main",
        "json-mapper/json-mapper": "^2",
        "json-mapper/symfony-bundle": "^2",
        "doctrine/orm": "*",
        "php": ">=8.1",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "symfony/console": "6.1.*",
        "symfony/dotenv": "6.1.*",
        "symfony/flex": "^2",
        "symfony/framework-bundle": "6.1.*",
        "symfony/runtime": "6.1.*",
        "symfony/yaml": "6.1.*",
        "react/http": "^1.3"
    },
    "autoload": {
        "psr-4": {
            "Ajo\\Tdd\\Examples\\Marketplace\\": "src/"
        },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5.13",
        "symfony/phpunit-bridge": "^6.1"
    },
    "autoload-dev": {
        "psr-4": {
            "Ajo\\Tdd\\Examples\\Tests\\": "Tests/"
        }
    },
    "config": {
        "allow-plugins": {
            "composer/package-versions-deprecated": true,
            "symfony/flex": true,
            "symfony/runtime": true
        },
        "optimize-autoloader": true,
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "replace": {
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php73": "*",
        "symfony/polyfill-php74": "*",
        "symfony/polyfill-php80": "*",
        "symfony/polyfill-php81": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "6.1.*"
        }
    }
}
